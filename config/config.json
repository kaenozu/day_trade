{
  "application": {
    "name": "Day Trade Personal",
    "version": "3.0.0",
    "environment": "development"
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "logs/application.log"
  },
  "database": {
    "url": "sqlite:///data/trading.db",
    "timeout": 30,
    "backup": {
      "enabled": true,
      "interval_hours": 24
    }
  },
  "trading": {
    "enabled": true,
    "market_hours": {
      "start": "09:00",
      "end": "15:00"
    },
    "risk_management": {
      "max_position_size": 10.0,
      "stop_loss_percentage": 3.0
    }
  },
  "performance": {
    "cache_enabled": true,
    "cache_ttl": 1800,
    "max_workers": 4
  }
}