{
  "version": "1.0.0",
  "name": "DayTrade Auto Settings",
  "description": "全自動デイトレード基本設定ファイル",
  
  "market_hours": {
    "pre_market": "08:00",
    "market_open": "09:00",
    "lunch_break_start": "11:30",
    "lunch_break_end": "12:30",
    "market_close": "15:00",
    "after_market": "15:30"
  },

  "database": {
    "url": "sqlite:///data/daytrade.db",
    "echo": false,
    "pool_pre_ping": true
  },

  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "logs/daytrade.log",
    "structured": true
  },

  "cache": {
    "enabled": true,
    "ttl_seconds": 300,
    "max_size": 1000
  },

  "development": {
    "debug": false,
    "mock_data": false,
    "test_mode": false
  }
}