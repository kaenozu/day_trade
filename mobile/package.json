{
  "name": "day-trade-ml-mobile",
  "version": "1.0.0",
  "description": "Day Trade ML System - React Native Mobile App",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build": "eas build",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "clean": "rm -rf node_modules && rm -rf .expo && npm install",
    "eject": "expo eject"
  },
  "dependencies": {
    "expo": "~49.0.15",
    "expo-status-bar": "~1.6.0",
    "expo-font": "~11.4.0",
    "expo-splash-screen": "~0.20.5",
    "expo-notifications": "~0.20.1",
    "expo-device": "~5.4.0",
    "expo-constants": "~14.4.2",
    "expo-secure-store": "~12.3.1",
    "expo-local-authentication": "~13.4.1",
    "expo-permissions": "~14.2.1",
    "expo-camera": "~13.4.4",
    "expo-barcode-scanner": "~12.5.3",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-paper": "^5.11.1",
    "react-native-vector-icons": "^10.0.2",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/drawer": "^6.6.6",
    "react-native-screens": "~3.22.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-svg": "13.9.0",
    "react-query": "^3.39.3",
    "axios": "^1.6.2",
    "socket.io-client": "^4.7.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-toast-message": "^2.1.6",
    "@react-native-async-storage/async-storage": "1.18.2",
    "react-native-keychain": "^8.1.3",
    "react-native-device-info": "^10.11.0",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-share": "^9.4.1",
    "react-native-fs": "^2.20.0",
    "react-native-image-picker": "^7.0.3",
    "react-native-image-crop-picker": "^0.40.2",
    "react-native-background-task": "^0.2.1",
    "react-native-push-notification": "^8.1.1",
    "@react-native-community/netinfo": "9.3.10",
    "react-native-mmkv": "^2.10.2",
    "react-native-flipper": "^0.212.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "react-hook-form": "^7.48.2",
    "zod": "^3.22.4",
    "@hookform/resolvers": "^3.3.2",
    "zustand": "^4.4.7",
    "immer": "^10.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.6",
    "@types/lodash": "^4.14.202",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.3",
    "@testing-library/react-native": "^12.4.2",
    "@testing-library/jest-native": "^5.4.3",
    "detox": "^20.13.5"
  },
  "resolutions": {
    "@types/react": "~18.2.14"
  },
  "overrides": {
    "@types/react": "~18.2.14"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-vector-icons|react-native-chart-kit|react-native-svg)/)"
    ]
  },
  "private": true,
  "expo": {
    "name": "Day Trade ML",
    "slug": "day-trade-ml",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.daytrademl.mobile",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": ["background-processing", "background-fetch"],
        "NSFaceIDUsageDescription": "生体認証でアプリのセキュリティを向上させるために使用します",
        "NSCameraUsageDescription": "QRコード読み取りのためにカメラを使用します",
        "NSMicrophoneUsageDescription": "音声によるアラート通知のために使用します"
      }
    },
    "android": {
      "package": "com.daytrademl.mobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#2563eb",
      "sounds": ["./assets/sounds/trade-alert.wav", "./assets/sounds/error-alert.wav"]
    },
    "extra": {
      "eas": {
        "projectId": "day-trade-ml-mobile"
      }
    },
    "plugins": [
      "expo-font",
      "expo-notifications",
      "expo-secure-store",
      "expo-local-authentication",
      [
        "expo-camera",
        {
          "cameraPermission": "QRコード読み取りのためにカメラへのアクセスが必要です"
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "QRコード読み取りのためにカメラへのアクセスが必要です"
        }
      ]
    ]
  }
}